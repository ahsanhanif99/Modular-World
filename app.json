{
  "expo": {
    "name": "ModularWorld",
    "slug": "modularworld",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "cpm.modular.worldtv",
      "buildNumber": "4",
      "infoPlist": {
        "NSCameraUsageDescription": "Modular World needs access to your camera to take photos.",
        "NSPhotoLibraryUsageDescription": "Modular World requires access to your photos to upload images.",
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": ["com.modular.worldtv"]
          }
        ]
      }
    },
    "android": {
      "package": "cpm.modular.worldtv",
      "intentFilters": [
        {
          "action": "VIEW",
          "category": ["BROWSABLE", "DEFAULT"],
          "data": {
            "scheme": "https",
            "host": "cpm.modular.worldtv"
          }
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "0e30b2bc-3f6e-4ccb-b163-5fbfebd99a6c"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/0e30b2bc-3f6e-4ccb-b163-5fbfebd99a6c"
    }
  }
}
